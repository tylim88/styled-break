{
	"name": "styled-break",
	"version": "1.0.0",
	"description": "üç® Create Responsive Styled Components With Breeze!",
	"main": "build/styledBreak.js",
	"files": [
		"build/styledBreak.js"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tylim88/styled-break"
	},
	"scripts": {
		"install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm i",
		"test": "jest",
		"coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"prepublishOnly": "npm run build -- --no-demo",
		"start": "webpack-dev-server --mode development --open",
		"build": "webpack --mode production",
		"lint": "eslint --cache --format codeframe --ext mjs,jsx,js src",
		"lint:fix": "npm run lint --fix"
	},
	"keywords": [
		"react",
		"styled-components",
		"css",
		"responsive",
		"breakpoint",
		"width"
	],
	"license": "MIT",
	"peerDependencies": {
		"styled-components": "^5"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.8.3",
		"@neutrinojs/react-components": "^9.1.0",
		"@typescript-eslint/eslint-plugin": "^2.19.0",
		"@typescript-eslint/parser": "^2.19.0",
		"babel-eslint": "^10.0.3",
		"babel-jest": "^25.1.0",
		"eslint": "^6.8.0",
		"eslint-cli": "^1.1.1",
		"eslint-config-prettier": "^6.10.0",
		"eslint-config-react-app": "^5.2.0",
		"eslint-plugin-flowtype": "^3.13.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.18.3",
		"eslint-plugin-react-hooks": "^1.7.0",
		"husky": "^4.2.1",
		"jest": "^25.1.0",
		"jest-styled-components": "^7.0.0",
		"neutrino": "^9.1.0",
		"prettier": "^1.19.1",
		"prettier-quick": "0.0.5",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-test-renderer": "^16.12.0",
		"styled-components": "^5.0.1",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.10.3"
	},
	"dependencies": {},
	"eslintConfig": {
		"extends": [
			"react-app",
			"prettier/react",
			"eslint:recommended",
			"plugin:prettier/recommended",
			"prettier"
		],
		"rules": {
			"no-unused-vars": [
				"error",
				{
					"args": "after-used"
				}
			]
		},
		"settings": {
			"import/resolver": {
				"node": {
					"moduleDirectory": [
						"node_modules",
						"src/"
					]
				}
			}
		}
	},
	"prettier": {
		"eslintIntegration": true,
		"singleQuote": true,
		"jsxSingleQuote": true,
		"trailingComma": "es5",
		"semi": false,
		"jsxBracketSameLine": false,
		"useTabs": true
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		]
	}
}
